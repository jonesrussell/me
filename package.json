{
	"name": "me",
	"version": "0.1.0",
	"homepage": "https://jonesrussell.github.io/me/",
	"type": "module",
	"scripts": {
		"dev": "NODE_ENV=development vite dev --host",
		"build": "NODE_ENV=production vite build",
		"preview": "NODE_ENV=production vite preview --host",
		"test": "NODE_ENV=development npm run test:integration && NODE_ENV=development npm run test:unit",
		"check": "NODE_ENV=development svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "NODE_ENV=development svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "NODE_ENV=development prettier --check . && NODE_ENV=development eslint .",
		"format": "NODE_ENV=development prettier --write .",
		"test:integration": "NODE_ENV=development playwright test",
		"test:unit": "NODE_ENV=development vitest"
	},
	"devDependencies": {
		"@fontsource/fira-mono": "^5.0.13",
		"@neoconfetti/svelte": "^2.2.1",
		"@playwright/test": "^1.45.2",
		"@sveltejs/adapter-auto": "^3.3.1",
		"@sveltejs/adapter-static": "^3.0.6",
		"@sveltejs/kit": "^2.9.0",
		"@sveltejs/vite-plugin-svelte": "^4.0.0-next.6",
		"@testing-library/jest-dom": "^6.4.6",
		"@testing-library/svelte": "^5.2.0",
		"@testing-library/user-event": "^14.5.2",
		"@types/eslint": "^8.56.10",
		"@types/jest": "^29.5.12",
		"and": "^0.0.3",
		"eslint": "^9.7.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.42.0",
		"globals": "^15.8.0",
		"jsdom": "^24.1.0",
		"prettier": "^3.3.3",
		"prettier-plugin-svelte": "^3.2.5",
		"svelte": "^5.0.0",
		"svelte-check": "^4.1.0",
		"tslib": "^2.6.3",
		"typescript": "^5.5.3",
		"typescript-eslint": "^8.0.0-alpha.20",
		"vite": "^5.4.6",
		"vitest": "^2.0.3"
	},
	"overrides": {
		"@sveltejs/vite-plugin-svelte": "^4.0.0-next.6"
	}
}
